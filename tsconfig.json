{
  "include": ["src/*.tsx","src/*.ts"],
  "compilerOptions": {
    "outFile": "lib/app.js",
    "strict": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "target": "es2022",
    "jsx": "react",
    "jsxFactory": "El",
    "moduleResolution": "node",
    "removeComments": true
  }
}
